<table class="table table-bordered table-hover">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Image</th>
    </tr>
    @foreach (Trainee item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            @* <td>@item.ImageUrl</td> *@
            <td><img src="~/Images/@item.ImageUrl" style="width:100px" /></td>
            <td>
                <a href="/trainee/Details/@item.Id">DEtails</a>
            </td>
        </tr>
    }
</table>
